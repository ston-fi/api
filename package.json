{
  "name": "@ston-fi/api",
  "version": "0.29.0",
  "description": "Typescript client for Ston.fi Http API",
  "license": "MIT",
  "homepage": "https://github.com/ston-fi/api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/api.git"
  },
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "cleanup": "rm -rf coverage dist node_modules build-report-*",
    "dev": "tsdown --sourcemap --watch ./src",
    "build": "tsdown",
    "format": "pnpm run /^format:.*/",
    "format:biome": "biome format --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:tsc": "tsc --noEmit --pretty",
    "lint:biome": "biome check",
    "test": "vitest",
    "prepublishOnly": "pnpm run lint && pnpm run test --run && pnpm run build --attw --publint"
  },
  "dependencies": {
    "camelcase-keys": "9.1.3",
    "decamelize-keys": "2.0.1",
    "ofetch": "1.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "lefthook": "2.0.2",
    "rollup-plugin-visualizer": "6.0.5",
    "tsdown": "0.16.1",
    "typescript": "5.9.3",
    "vitest": "4.0.8"
  }
}

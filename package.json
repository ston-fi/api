{
  "name": "@ston-fi/api",
  "version": "0.24.0",
  "description": "Typescript client for Ston.fi Http API",
  "license": "MIT",
  "homepage": "https://github.com/ston-fi/api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/api.git"
  },
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bare": {
        "types": "./dist/bare/index.d.ts",
        "default": "./dist/bare/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "cleanup": "rm -rf coverage dist node_modules build-report-*",
    "format": "pnpm run /^format:.*/",
    "format:biome": "biome format --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:tsc": "tsc --noEmit --pretty",
    "lint:biome": "biome check",
    "test": "vitest",
    "build": "tsup",
    "prepublishOnly": "pnpm run format && pnpm run lint && pnpm run test --run && pnpm run build && pnpm pack && pnpm attw --pack *.tgz --profile node16 && rm *.tgz && pnpm publint"
  },
  "dependencies": {
    "humps": "2.0.1",
    "ofetch": "1.4.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "type-fest": "4.41.0",
    "@biomejs/biome": "2.0.4",
    "@types/humps": "^2.0.6",
    "esbuild-analyzer": "0.2.0",
    "lefthook": "1.11.14",
    "publint": "0.3.12",
    "tsup": "8.3.6",
    "typescript": "^5",
    "vitest": "3.2.4"
  }
}
